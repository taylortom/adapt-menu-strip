<div class="menu-container" role="region" aria-label="{{_globals._menu._stripmenu.ariaRegion}}">
	<div class='menu-container-inner strip-menu-inner clearfix'>
		<div class="menu-header">
			<div class="menu-header-inner">
				{{#if displayTitle}}
				<div class="menu-title">
					<div class="menu-title-inner h1" role="heading" aria-level="1" tabindex="0">
						{{{compile displayTitle}}}
					</div>
				</div>
				{{/if}}
				{{#if body}}
				<div class="menu-body">
					<div class="menu-body-inner">
						{{{compile_a11y_text body}}}
					</div>
				</div>
				{{/if}}
			</div>
		</div>
	</div>
	<div class="aria-label relative a11y-ignore-focus prevent-default" tabindex="0" role="region">{{_globals._menu._stripmenu.menuEnd}}</div>
</div>
